    - Poisonous Snake
    - Pony
    - Rat
    - Raven
    - Swarm of Insects
    - Swarm of rats
    - Swarm of ravens
    - Vulture
    - Weasel
    - Wolf
}
->->


=== create_encounter ===
{encounter_difficulty == (): -> edit_encounter_difficulty ->}
{location == (): -> edit_location ->}
{location == wilderness && terrain == (): -> edit_terrain ->}

// Creating {encounter_difficulty} encounter for {stat_party_size} lvl {stat_level} PCs in {DisplayLocation()}.
-> roll_encounter -> display_encounter ->->


=== create_monster(difficult) ===
~ encounter_difficulty = difficulty
-> roll_encounter -> display_encounter ->->

=== display_encounter ===

{monster_count > 0:
    -> roll_monster(monster_level, monster_count) ->
}

{monster_count_2 > 0:
    <> and <>
    -> roll_monster(monster_level, monster_count_2) ->
}

->->


=== create_single_monster ===
~ encounter_difficulty = medium
{location == (): -> edit_location ->}
{location == wilderness && terrain == (): -> edit_terrain ->}

-> roll_encounter -> roll_monster(monster_level, 1) ->->


=== edit_encounter_difficulty ===
What difficulty is this encounter?

+ [Easy]
    ~ encounter_difficulty = easy

+ [Medium]
    ~ encounter_difficulty = medium

+ [Hard]
    ~ encounter_difficulty = hard

+ [Deadly]
    ~ encounter_difficulty = deadly
    
+ [Random]
    ~ encounter_difficulty = LIST_RANDOM(LIST_ALL(encounter_difficulty))
    
- ->->


=== create_boss_monster ===
TODO: Boss Monster Creator
Boss Monster
->->

=== function is_encounter_supported ===
{stat_party_size == 1:
    ~ return stat_level <= 3
}

{stat_party_size == 2:
    ~ return stat_level <= 2
}

~ return false

=== roll_encounter ===
~ monster_count = 0
~ monster_level = ()

~ monster_count_2 = 0
~ monster_level_2 = ()

{
    - stat_party_size == 1:
        -> solo ->
    - stat_party_size == 2:
        -> duo ->
    - else:
        ~logError("Cannot create encounters for a party of {stat_party_size}. Enounters currently only support parties of 1 or 2.")
}
->->

= solo
{stat_level:
    - 1:
        {encounter_difficulty:
            - easy:   -> L1Easy ->
            - medium: -> L1Medium ->
            - hard:   -> L1Hard ->
            - deadly: -> L1Deadly ->
        }
    - 2:
        {encounter_difficulty:
            - easy:   -> L2Easy ->
            - medium: -> L2Medium ->
            - hard:   -> L2Hard ->
            - deadly: -> L2Deadly ->
        }
    - 3:
        {encounter_difficulty:
            - easy:   -> L3Easy ->
            - medium: -> L3Medium ->
            - hard:   -> L3Hard ->
            - deadly: -> L3Deadly ->
        }
    - else:
        ~logError("Cannot create encounters for a party of 1 at level {stat_level}. Must be level 1, 2, or 3.")
}

->->

= duo
{stat_level:
    - 1:
        {encounter_difficulty:
            - easy:   -> L1Medium ->
            - medium: -> L1Deadly ->
            - hard:   -> L2Hard ->
            - deadly: -> L2Deadly ->
        }
    - 2:
        {encounter_difficulty:
            - easy:   -> L2Medium ->
            - medium: -> L2Deadly ->
            - hard:   -> L6Easy ->
            - deadly: -> L3Deadly ->
        }
    - else:
        ~logError("Cannot create encoutners for a party o 2 at level {stat_level}. Must be level 1 or 2.")
}

->->

= L1Easy
{d4() <= 2:
    ~ monster_count = 1
    ~ monster_level = cr0
- else:
    ~ monster_count = 2
    ~ monster_level = cr0
}
->->

= L1Medium
~ roll = d4()
{
    - roll <= 2:
        ~ monster_count = 1
        ~ monster_level = cr1_8
    - else:
        ~ monster_count = 2
        ~ monster_level = cr0
}
->->

= L1Hard
~ roll = d6()
{
    - roll <= 2:
        ~ monster_count = 1
        ~ monster_level = cr1_8
        ~ monster_count_2 = 1
        ~ monster_level_2 = cr0
    - roll <= 4:
        ~ monster_count = 1
        ~ monster_level = cr1_4
    - else:
        ~ monster_count = 3
        ~ monster_level = cr0
}
->->

= L1Deadly
~ roll = d6()
{
    - roll <= 2:
        ~ monster_count = 2
        ~ monster_level = cr1_8
    - roll <= 4:
        ~ monster_count = 4
        ~ monster_level = cr0
    - else:
        ~ monster_count = 1
        ~ monster_level = cr1_4
        ~ monster_count_2 = 1
        ~ monster_level_2 = cr0
}
->->

= L2Easy
{d4() <= 2:
    ~ monster_count = 1
    ~ monster_level = cr1_8
- else:
    ~ monster_count = 2
    ~ monster_level = cr0
}
->->

= L2Medium
~ roll = d6()
{
    - roll <= 2:
        ~ monster_count = 2
        ~ monster_level = cr1_8
    - roll <= 4:
        ~ monster_count = 4
        ~ monster_level = cr0
    - else:
        ~ monster_count = 1
        ~ monster_level = cr1_4
        ~ monster_count_2 = 1
        ~ monster_level_2 = cr0
}
->->

= L2Hard
~ roll = d6()
{
    - roll <= 2:
        ~ monster_count = 1
        ~ monster_level = cr1_2
    - roll <= 4:
        ~ monster_count = 1
        ~ monster_level = cr1_4
        ~ monster_count_2 = 1
        ~ monster_level_2 = cr1_8
    - else:
        ~ monster_count = 2
        ~ monster_level = cr1_8
        ~ monster_count_2 = 1
        ~ monster_level_2 = cr0
}
->->

= L2Deadly
~ roll = d4()
{
    - roll <= 1:
        ~ monster_count = 3
        ~ monster_level = cr1_8
    - roll <= 2:
        ~ monster_count = 2
        ~ monster_level = cr1_4
    - roll <= 3:
        ~ monster_count = 1
        ~ monster_level = cr1_4
        ~ monster_count_2 = 3
        ~ monster_level_2 = cr0
    - else:
        ~ monster_count = 3
        ~ monster_level = cr1_8
        ~ monster_count_2 = 1
        ~ monster_level_2 = cr0
}
->->

= L3Easy
~ roll = d6()
{
    - roll <= 2:
        ~ monster_count = 1
        ~ monster_level = cr1_8
        ~ monster_count_2 = 1
        ~ monster_level_2 = cr0
    - roll <= 4:
        ~ monster_count = 1
        ~ monster_level = cr1_4
    - else:
        ~ monster_count = 3
        ~ monster_level = cr0
}
->->

= L3Medium
~ roll = d6()
{
    - roll <= 2:
        ~ monster_count = 1
        ~ monster_level = cr1_2
    - roll <= 4:
        ~ monster_count = 1
        ~ monster_level = cr1_4
        ~ monster_count_2 = 1
        ~ monster_level_2 = cr1_8
    - else:
        ~ monster_count = 2
        ~ monster_level = cr1_8
        ~ monster_count_2 = 1
        ~ monster_level_2 = cr0
}
->->

= L3Hard
~ roll = d4()
{
    - roll <= 1:
        ~ monster_count = 1
        ~ monster_level = cr1_4
        ~ monster_count_2 = 4
        ~ monster_level_2 = cr0
    - roll <= 2:
        ~ monster_count = 3
        ~ monster_level = cr1_8
        ~ monster_count_2 = 2
        ~ monster_level_2 = cr0
    - roll <= 3:
        ~ monster_count = 2
        ~ monster_level = cr1_8
        ~ monster_count_2 = 1
        ~ monster_level_2 = cr1_4
    - else:
        ~ monster_count = 1
        ~ monster_level = cr1
}
->->

= L3Deadly
~ roll = d6()
{
    - roll <= 2:
        ~ monster_count = 1
        ~ monster_level = cr1_2
        ~ monster_count_2 = 2
        ~ monster_level_2 = cr1_8
    - roll <= 4:
        ~ monster_count = 2
        ~ monster_level = cr1_2
    - else:
        ~ monster_count = 1
        ~ monster_level = cr1_2
        ~ monster_count_2 = 2
        ~ monster_level_2 = cr1_8
}
->->




= L6Easy
~ roll = d6()
{
    - roll <= 2:
        ~ monster_count = 1
        ~ monster_level = cr1
    - roll <= 4:
        ~ monster_count = 1
        ~ monster_level = cr1_2
        ~ monster_count_2 = 1
        ~ monster_level_2 = cr1_4
    - else:
        ~ monster_count = 2
        ~ monster_level = cr1_4
        ~ monster_count_2 = 1
        ~ monster_level_2 = cr1_8
}
->->


=== roll_monster(cr, num) ===

{
    - cr == cr0:   -> cr0_table(num) ->
    - cr == cr1_8: -> cr1_8_table(num) ->
    - cr == cr1_4: -> cr1_4_table(num) ->
    - cr == cr1_2: -> cr1_2_table(num) ->
    - cr == cr1:   -> cr1_table(num) ->
    - cr == cr2:   -> cr2_table(num) ->
    - cr == cr3:   -> cr3_table(num) ->
    - cr == cr4:   -> cr4_table(num) ->
    - cr == cr5:   -> cr5_table(num) ->
    - cr == cr6:   -> cr6_table(num) ->
    - cr == cr7:   -> cr7_table(num) ->
    - else: ~logError("Unsupported cr: {cr} in {DisplayTerrain()}")
}

->->


=== cr0_table(num) ===
~ temp s = num > 1
{num} <>

{
    - location == dungeon: -> cr0_dungeon(s) ->
    - location == urban:   -> cr0_urban(s) ->
    - terrain == aquatic:  -> cr0_aquatic(s) ->
    - terrain == coastal:  -> cr0_coastal(s) ->
    - terrain == desert:   -> cr0_desert(s) ->
    - terrain == forest:   -> cr0_forest(s) ->
    - terrain == grass:    -> cr0_grass(s) ->
    - terrain == hill:     -> cr0_hill(s) ->
    - terrain == mountain: -> cr0_mountain(s) ->
    - terrain == swamp:    -> cr0_swamp(s) ->
    
    - else: ~logError("Unsupported: cr0 monster in {DisplayLocation()}")
}

->->


=== cr1_8_table(num) ===
~ temp s = num > 1
{num} <>

{
    - location == dungeon: -> cr1_8_dungeon(s) ->
    - location == urban:   -> cr1_8_urban(s) ->
    - terrain == aquatic:  -> cr1_8_aquatic(s) ->
    - terrain == coastal:  -> cr1_8_coastal(s) ->
    - terrain == desert:   -> cr1_8_desert(s) ->
    - terrain == forest:   -> cr1_8_forest(s) ->
    - terrain == grass:    -> cr1_8_grass(s) ->
    - terrain == hill:     -> cr1_8_hill(s) ->
    - terrain == mountain: -> cr1_8_mountain(s) ->
    - terrain == swamp:    -> cr1_8_swamp(s) ->

    - else: ~logError("Unsupported: cr0 monster in {DisplayLocation()}")
}

->->


